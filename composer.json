{
  "name" : "theseer/phpdox",
  "description" : "A fast Documentation generator for PHP Code using standard technology (SRC, DOCBLOCK, XML and XSLT) with event based processing",
  "license" : "BSD-3-Clause",
  "authors" : [
    {
      "name" : "Arne Blankerts",
      "email" : "arne@blankerts.de",
      "role" : "Developer"
    }
  ],
  "support" : {
    "issues" : "https://github.com/theseer/phpdox/issues"
  },
  "require" : {
    "php": ">=7.1",
    "ext-dom": "*",
    "ext-fileinfo": "*",
    "ext-iconv": "*",
    "ext-mbstring": "*",
    "ext-tokenizer": "*",
    "ext-xsl": "*",
    "nikic/php-parser": "^4.2",
    "phpunit/php-timer": "^2.0|^5.0",
    "theseer/directoryscanner": "^1.3.0",
    "theseer/fdomdocument": "^1.6",
    "theseer/fxsl": "^1.1"
  },
  "autoload": {
    "classmap": [
      "src/"
    ]
  },
  "bin" : [
      "phpdox"
  ],
  "minimum-stability" : "dev",
  "prefer-stable" : true
}
